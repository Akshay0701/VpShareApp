<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".commanderacivty.CommanderDashBorad">
    <!--this is upper image  -->
    <LinearLayout
        android:layout_width="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="40dp"
            android:id="@+id/homeBtn"
            android:src="@drawable/homeicon"
            android:layout_marginLeft="0dp"
            android:layout_height="50dp"></ImageView>
        <ImageView
            android:layout_width="40dp"
            android:id="@+id/Call"
            android:src="@drawable/callicon"
            android:layout_marginLeft="30dp"
            android:layout_height="50dp"></ImageView>
        <ImageView
            android:layout_width="40dp"
            android:id="@+id/Website"
            android:src="@drawable/websiteicon"
            android:layout_marginLeft="30dp"
            android:layout_height="50dp"></ImageView>
        <ImageView
            android:layout_width="40dp"
            android:id="@+id/Location"
            android:src="@drawable/locationicon"
            android:layout_marginLeft="30dp"
            android:layout_height="50dp"></ImageView>
        <ImageView
            android:layout_width="40dp"
            android:id="@+id/FaceBook"
            android:src="@drawable/facebookicon"
            android:padding="5dp"
            android:layout_marginLeft="30dp"
            android:layout_height="50dp"></ImageView>
        <ImageView
            android:layout_width="40dp"
            android:id="@+id/Instagram"
            android:src="@drawable/instagramicon"
            android:layout_marginLeft="30dp"
            android:layout_height="50dp"></ImageView>
    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/text"
        android:text="Your Not Authorized By Admin"></TextView>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:id="@+id/content1">
    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        app:itemBackground="@color/colorAccent"
        app:menu="@menu/commander_nav" />


</LinearLayout>